<?xml version="1.0" encoding="UTF-8"?>
<customUI onLoad="Ribbon_Load" xmlns="http://schemas.microsoft.com/office/2006/01/customui">
    <ribbon>
        <tabs>
            <tab id="PdfPasswordRemover" label="PdfPasswordRemover">
                <group id="grp">
                    <button id="RemovePasswordBtn" onAction="OnRemovePwd" label="Remove Password" size="large" getImage="GetUnlockIcon" />
                    <button id="SetPasswordBtn" onAction="OnSetPwd" label="Set Password" size="large" getImage="GetPwdIcon" />
                </group>
            </tab>
        </tabs>
    </ribbon>
</customUI>